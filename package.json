{
	"name": "project1-auth",
	"version": "1.0.0",
	"description": "LoopBack",
	"keywords": [
		"loopback-application",
		"loopback"
	],
	"main": "index.js",
	"engines": {
		"node": ">=8.9"
	},
	"scripts": {
		"build": "lb-tsc",
		"build:watch": "lb-tsc --watch",
		"clean": "lb-clean dist *.tsbuildinfo",
		"lint": "npm run prettier:check && npm run eslint",
		"lint:fix": "npm run eslint:fix && npm run prettier:fix",
		"prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
		"prettier:check": "npm run prettier:cli -- -l",
		"prettier:fix": "npm run prettier:cli -- --write",
		"eslint": "lb-eslint --report-unused-disable-directives .",
		"eslint:fix": "npm run eslint --debug --fix",
		"pretest": "npm run clean && npm run build",
		"test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
		"posttest": "npm run lint",
		"test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
		"docker:build": "docker build -t project1-auth .",
		"docker:run": "docker run -p 3000:3000 -d project1-auth",
		"migrate": "node ./dist/migrate",
		"prestart": "npm run clean && npm run build",
		"start": "node -r source-map-support/register .",
		"prepublishOnly": "npm run test",
		"db:migrate:up": "db-migrate up --config './migrations/database.json' -e live",
		"db:migrate:down": "db-migrate down --config './migrations/database.json' -e live",
		"db:migrate:reset": "db-migrate reset --config './migrations/database.json' -e live",
		"db:migrate:up:test": "db-migrate up --config './migrations/database.json' -e test",
		"db:migrate:down:test": "db-migrate down --config './migrations/database.json' -e test",
		"db:migrate:reset:test": "db-migrate reset --config './migrations/database.json' -e test"
	},
	"repository": {
		"type": "git"
	},
	"author": "KDA",
	"license": "",
	"files": [
		"README.md",
		"index.js",
		"index.d.ts",
		"dist",
		"src",
		"!*/__tests__"
	],
	"dependencies": {
		"@loopback/boot": "^1.7.2",
		"@loopback/context": "^2.0.0",
		"@loopback/core": "^1.12.2",
		"@loopback/openapi-v3": "^1.12.0",
		"@loopback/repository": "^1.18.0",
		"@loopback/rest": "^1.26.0",
		"@loopback/rest-explorer": "^1.4.8",
		"@loopback/service-proxy": "^1.3.15",
		"bcrypt": "^3.0.6",
		"db-migrate": "^0.11.6",
		"db-migrate-pg": "^1.0.0",
		"dotenv": "^8.2.0",
		"jsonwebtoken": "^8.5.1",
		"loopback-connector-kv-redis": "^3.0.2",
		"loopback-connector-postgresql": "^3.8.3",
		"loopback4-authentication": "^2.1.3",
		"loopback4-authorization": "^2.3.2"
	},
	"devDependencies": {
		"@loopback/build": "^3.1.0",
		"@loopback/eslint-config": "^5.0.2",
		"@loopback/testlab": "^1.10.2",
		"@loopback/tslint-config": "^2.1.0",
		"@types/bcrypt": "^3.0.0",
		"@types/jsonwebtoken": "^8.3.7",
		"@types/node": "^10.17.13",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-eslint-plugin": "^2.2.1",
		"eslint-plugin-mocha": "^6.2.2",
		"source-map-support": "^0.5.16",
		"typescript": "~3.7.5"
	}
}
